        <scriptName>Update row from time stamp field</scriptName>
        <scriptText><![CDATA[importPackage(Packages.org.csstudio.opibuilder.scriptUtil);

var convertLongArrayToIntArray = function(longArray) {
  var intArray = Packages.java.lang.reflect.Array.newInstance(Packages.java.lang.Integer.TYPE, longArray.length);
  for (var i = 0; i < longArray.length; ++i) {
    intArray[i] = (longArray[i] < 2147483648) ? longArray[i] : (longArray[i] - 4294967296);
  }
  return intArray;
};

var pvSelectedIndex = pvs[0];
var pvSelectedTimeStamp = pvs[1];
var pvTimeStamps = pvs[2];

var index = PVUtil.getLong(pvSelectedIndex).longValue();
var selectedTimeStamp = PVUtil.getLong(pvSelectedTimeStamp);
var timeStamps = PVUtil.getLongArray(pvTimeStamps);

// We have to convert from an array of longs to an array of
// ints because otherwise the write operation will fail.
timeStamps = convertLongArrayToIntArray(timeStamps);

if (index < timeStamps.length) {
  var storedTimeStamp = timeStamps[index];
  if (storedTimeStamp >= 2147483648) {
    storedTimeStamp -= 4294967296;
  }
  if (selectedTimeStamp >= 2147483648) {
    selectedTimeStamp -= 4294967296;
  }
  if (storedTimeStamp != selectedTimeStamp) {
    timeStamps[index] = selectedTimeStamp;
    PVUtil.writePV(pvTimeStamps.getName(), timeStamps);
  }
}
]]></scriptText>
