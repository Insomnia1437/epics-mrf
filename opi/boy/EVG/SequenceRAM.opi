        <scriptName>Update on PV change</scriptName>
        <scriptText><![CDATA[importPackage(Packages.org.csstudio.opibuilder.scriptUtil);

var rawTimeStamps = PVUtil.getLongArray(pvs[0]);
var rawEventCodes = PVUtil.getLongArray(pvs[1]);

var numberOfEntries = Math.min(rawTimeStamps.length, rawEventCodes.length);

var contentArray = Packages.java.lang.reflect.Array.newInstance(Packages.java.lang.reflect.Array.newInstance(Packages.java.lang.String, 0).getClass(), numberOfEntries);
for (var i = 0; i < numberOfEntries; ++i) {
  var entry = Packages.java.lang.reflect.Array.newInstance(Packages.java.lang.String, 3);
  entry[0] = i;
  entry[1] = rawTimeStamps[i] >= 0 ? rawTimeStamps[i] : (rawTimeStamps[i] + 4294967296);
  var eventCode = rawEventCodes[i] >= 0 ? rawEventCodes[i] : (rawEventCodes[i] + 256);
  switch(eventCode) {
  case 0x00:
    entry[2] = "Null";
    break;
  case 0x70:
    entry[2] = "Seconds 0";
    break;
  case 0x71:
    entry[2] = "Seconds 1";
    break;
  case 0x7a:
    entry[2] = "Heartbeat";
    break;
  case 0x7b:
    entry[2] = "Synchronize Prescalers";
    break;
  case 0x7c:
    entry[2] = "Timestamp Counter Increment";
    break;
  case 0x7d:
    entry[2] = "Timestamp Counter Reset";
    break;
  case 0x7f:
    entry[2] = "End of Sequence";
    break;
  default:
    var eventCodeString = "0x" + Math.floor(eventCode / 16).toString(16) + (eventCode % 16).toString(16);
    entry[2] = eventCodeString;
  }
  contentArray[i] = entry;
}

widget.getTable().setContent(contentArray);
]]></scriptText>
