      <path pathString="EmbeddedJs" checkConnect="true" sfe="false" seoe="false">
        <scriptName>Update fields on row change</scriptName>
        <scriptText><![CDATA[importPackage(Packages.org.csstudio.opibuilder.scriptUtil);

var pvSelectedIndex = pvs[0];
var pvSelectedTimeStamp = pvs[1];
var pvSelectedEventCodeType = pvs[2];
var pvSelectedEventCodeNumber = pvs[3];
var pvTimeStamps = pvs[4];
var pvEventCodes = pvs[5];

var index = PVUtil.getLong(pvSelectedIndex).longValue();
var timeStamps = PVUtil.getLongArray(pvTimeStamps);
var eventCodes = PVUtil.getLongArray(pvEventCodes);

if (index < Math.min(timeStamps.length, eventCodes.length)) {
  var timeStamp = (timeStamps[index] >= 0) ? timeStamps[index] : (timeStamps[index] + 4294967296);
  var eventCode = (eventCodes[index] >= 0) ? eventCodes[index] : (eventCodes[index] + 256);
  var eventCodeType;
  switch(eventCode) {
  case 0x00:
    eventCodeType = "Null";
    break;
  case 0x70:
    eventCodeType = "Seconds 0";
    break;
  case 0x71:
    eventCodeType = "Seconds 1";
    break;
  case 0x7a:
    eventCodeType = "Heartbeat";
    break;
  case 0x7b:
    eventCodeType = "Synchronize Prescalers";
    break;
  case 0x7c:
    eventCodeType = "Timestamp Counter Increment";
    break;
  case 0x7d:
    eventCodeType = "Timestamp Counter Reset";
    break;
  case 0x7f:
    eventCodeType = "End of Sequence";
    break;
  default:
    eventCodeType = "User Defined";
  }
  PVUtil.writePV(pvSelectedTimeStamp.getName(), timeStamp);
  PVUtil.writePV(pvSelectedEventCodeNumber.getName(), eventCode);
  PVUtil.writePV(pvSelectedEventCodeType.getName(), eventCodeType);
}
]]></scriptText>
